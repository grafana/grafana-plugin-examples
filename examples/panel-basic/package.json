{
  "name": "basic-panel",
  "version": "1.0.0",
  "description": "Showcase how to build a basic panel plugin",
  "scripts": {
    "build": "webpack -c ./webpack/webpack.prod.conf.js",
    "dev": "docker-compose up -d && webpack -w -c ./webpack/webpack.dev.conf.js",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn lint --fix"
  },
  "author": "Grafana Labs",
  "license": "Apache-2.0",
  "devDependencies": {
    "@grafana/eslint-config": "^2.5.0",
    "@grafana/tsconfig": "^1.0.0-rc1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.5.1",
    "webpack": "^5.55.1",
    "webpack-cli": "^4.8.0",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "resolutions": {
    "rxjs": "7.3.0"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@grafana/data": "latest",
    "@grafana/runtime": "latest",
    "@grafana/schema": "latest",
    "@grafana/toolkit": "latest",
    "@grafana/ui": "latest",
    "@types/lodash": "latest"
  }
}
